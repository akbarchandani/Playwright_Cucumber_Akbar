{
  "name": "playwright_cucumber_akbar",
  "version": "1.0.0",
  "description": "Cucumber Playwright Allure test framework",
  "main": "index.js",
  "scripts": {
    "test": "cucumber-js --require-module ts-node/register",
    "test:allure": "cucumber-js --format @allure-decorators/cucumberjs/dist/cjs --publish",
    "allure:generate": "allure generate allure-results --clean -o allure-report",
    "allure:open": "allure open allure-report"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  
  "dependencies": {
    "@cucumber/cucumber": "^12.1.0",
    "@playwright/test": "^1.54.1",
    "allure-commandline": "^2.34.1",
    "allure-cucumberjs": "^3.3.2",
    "playwright": "^1.54.1"
  }
}
